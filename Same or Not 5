#include <stdio.h>

int main() {

    int size, i, evenCount = 0, oddCount = 0;

    

    

    scanf("%d", &size);

    

    int arr[size];

    

    

    for(i = 0; i < size; i++) {

        scanf("%d", &arr[i]);

        

        

        if(arr[i] % 2 == 0)

            evenCount++;

        else

            oddCount++;

    }


    if(evenCount == size)

        printf("Even\n");

    else if(oddCount == size)

        printf("Odd\n");

    else

        printf("Mixed\n");

    return 0;

}
